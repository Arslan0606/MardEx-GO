<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Mardex" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/main_color</item>
        <item name="colorPrimaryVariant">@color/secondary_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/main_color</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/light_white</item>
        <item name="otpViewStyle">@style/OtpWidget.OtpView</item>

    </style>


    <style name="ImageViewDefStyle">
        <item name="android:padding">@dimen/_4sdp</item>
        <item name="android:layout_width">@dimen/_42sdp</item>
        <item name="android:layout_height">@dimen/_42sdp</item>
        <item name="android:adjustViewBounds">true</item>
    </style>

    <style name="TextMedium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/gilroy_semi_bold_600</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginBottom">@dimen/_8sdp</item>
    </style>

    <style name="TextLarge" parent="TextMedium">
        <item name="android:fontFamily">@font/gilroy_semi_bold_600</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextSmall" parent="TextMedium">
